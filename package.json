{
  "name": "jet-paths",
  "version": "2.0.0",
  "description": "Preprend strings in an object with the value from a base-key.",
  "type": "module",              
  "main": "./cjs/index.js",      
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "types": "./esm/index.d.ts"
    }
  },
  "files": [
    "cjs",
    "esm"
  ],
  "sideEffects": false,
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:test": "npm run clean && tsc -p tsconfig.test.json",
    "build": "npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist",
    "clean:install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "test": "npm run build:test && node --test dist/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/jet-paths.git"
  },
  "keywords": [
    "express",
    "path",
    "paths",
    "route",
    "routes",
    "supertest",
    "jasmine",
    "full",
    "fullpath",
    "fullpaths",
    "object"
  ],
  "author": "sean maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/jet-paths/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/jet-paths#readme",
  "devDependencies": {
    "tslib": "^2.6.3",
    "typescript": "~5.9.3"
  }
}
